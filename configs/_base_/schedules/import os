import os
import sys
paths = set(sys.argv[1:-1])
int rep = int(sys.argv[-1])
for path in sys.argv[1:-1]:
    parts = path.split('_')
    num = int(parts[1])
    for i in range(rep):
        step=i
        while True:
            parts[1] = '%05d' % (num+step)
            dst_path = '_'.join(parts)
            if not os.path.exists(dst_path):
                break
            step += 40
        print('cp %s %s' % (path, dst_path))